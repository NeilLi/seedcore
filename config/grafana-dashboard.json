{
  "dashboard": {
    "id": null,
    "title": "SeedCore Coordinator - Predicate System",
    "tags": ["seedcore", "coordinator", "predicates"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "OCPS Signals",
        "type": "stat",
        "targets": [
          {
            "expr": "coord_p_fast",
            "legendFormat": "p_fast"
          },
          {
            "expr": "coord_s_drift",
            "legendFormat": "s_drift"
          },
          {
            "expr": "coordinator:escalation_ratio",
            "legendFormat": "Escalation Ratio"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.6},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate by Path",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(coord_requests_total[5m])",
            "legendFormat": "{{path}} - {{success}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Energy Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "coord_deltaE_est",
            "legendFormat": "ΔE_est (Expected)"
          },
          {
            "expr": "coord_deltaE_realized",
            "legendFormat": "ΔE_realized (Observed)"
          },
          {
            "expr": "coordinator:deltaE_realized_avg",
            "legendFormat": "Avg ΔE Realized"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "GPU Guard Status",
        "type": "stat",
        "targets": [
          {
            "expr": "coord_gpu_guard_ok",
            "legendFormat": "Guard OK"
          },
          {
            "expr": "coord_gpu_queue_depth",
            "legendFormat": "Queue Depth"
          },
          {
            "expr": "coord_gpu_concurrent_jobs",
            "legendFormat": "Concurrent Jobs"
          },
          {
            "expr": "coord_gpu_budget_remaining_s",
            "legendFormat": "Budget Remaining (s)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Request Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(coord_e2e_ms_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(coord_e2e_ms_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(coord_e2e_ms_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Circuit Breaker Status",
        "type": "table",
        "targets": [
          {
            "expr": "coordinator:circuit_breaker_health",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Predicate Evaluation Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(coord_predicate_evaluations_total[5m])",
            "legendFormat": "{{rule_type}} - {{result}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Memory Synthesis Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "coordinator:memory_synthesis_success_rate",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Cost per ΔE (Efficiency)",
        "type": "graph",
        "targets": [
          {
            "expr": "coordinator:cost_per_deltaE",
            "legendFormat": "Cost per ΔE"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Error Budget (SLO)",
        "type": "graph",
        "targets": [
          {
            "expr": "coordinator:error_budget_remaining",
            "legendFormat": "Error Budget Remaining"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
