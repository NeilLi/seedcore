      ]
    }
  ],
  "entity_patterns": [
    {
      "id": "temperature_entity",
      "entity_type": "CUSTOM/TEMPERATURE",
      "patterns": [
        {
          "regex": "\\b\\d+(?:\\.\\d+)?\\s*(?:°\\s*[CF]|degrees?)\\b",
          "context": "(?:temperature|temp|hvac|ac|heating|cooling)"
        }
      ],
      "priority": 60,
      "confidence": 0.9,
      "event_types": [
        "hvac"
      ],
      "description": "Temperature value extraction",
      "enabled": true,
      "emits_attributes": {
        "value_hint": "temperature"
      }
    },
    {
      "id": "room_number_entity",
      "entity_type": "CUSTOM/ROOM",
      "patterns": [
        {
          "regex": "\\b(?:room|rm|suite)\\s*[-#:]?\\s*[A-Za-z0-9]{2,6}\\b"
        }
      ],
      "priority": 55,
      "confidence": 0.8,
      "event_types": [
        "hvac",
        "security",
        "maintenance",
        "vip"
      ],
      "description": "Room/suite number extraction",
      "enabled": true
    },
    {
      "id": "currency_amount",
      "entity_type": "MONEY",
      "patterns": [
        {
          "regex": "\\b(?:USD|EUR|GBP|\\$|€|£)\\s?\\d{1,3}(?:[,\\s]\\d{3})*(?:\\.\\d{1,2})?\\b"
        }
      ],
      "priority": 70,
      "confidence": 0.85,
      "event_types": [
        "fraud",
        "maintenance",
        "vip"
      ],
      "description": "Currency amounts",
      "enabled": true
    },
    {
      "id": "account_reference",
      "entity_type": "CUSTOM/ACCOUNT",
      "patterns": [
        {
          "regex": "\\b(?:acct|account|iban|pan)[:\\s-]?[A-Za-z0-9]{6,34}\\b"
        }
      ],
      "priority": 65,
      "confidence": 0.8,
      "event_types": [
        "fraud"
      ],
      "description": "Financial account references",
      "enabled": true
    },
    {
      "id": "device_id",
      "entity_type": "CUSTOM/DEVICE",
      "patterns": [
        {
          "regex": "\\b(dev|device|serial|sn)[:\\s-]?[A-Za-z0-9\\-]{6,32}\\b"
        }
      ],
      "priority": 60,
      "confidence": 0.8,
      "event_types": [
        "robotics",
        "maintenance",
        "security"
      ],
      "description": "Device or serial identifiers",
      "enabled": true
    },
    {
      "id": "time_entity",
      "entity_type": "TIME",
      "patterns": [
        {
          "regex": "\\b(?:\\d{1,2}:\\d{2}(?:\\s*[AP]M)?|\\d{1,2}\\s*(?:am|pm|AM|PM))\\b"
        }
      ],
      "priority": 45,
      "confidence": 0.7,
      "event_types": [
        "maintenance",
        "vip",
        "emergency"
      ],
      "description": "Time mentions",
      "enabled": true
    }
  ],
  "composite_rules": [
    {
      "id": "vip_privacy_baseline",
      "any_of": [
        "vip_intent",
        "vip_keywords"
      ],
      "emits_tags": [
        "privacy",
        "vip"
      ],
      "emits_attributes": {
        "privacy_mode": "STRICT"
      },
      "priority": 120,
      "enabled": true
    },
    {
      "id": "allergen_or_emergency",
      "any_of": [
        "allergen_exposure",
        "emergency_signal"
      ],
      "emits_tags": [
        "emergency"
      ],
      "priority": 110,
      "enabled": true
    },
    {
      "id": "robotics_fault_high",
      "all_of": [
        "robotics_fault",
        "maintenance_need"
      ],
      "emits_tags": [
        "maintenance_request"
      ],
      "emits_attributes": {
        "priority": "critical"
      },
      "priority": 130,
      "enabled": true
    }
  ],
  "keyword_dictionaries": [
    {
      "name": "DEPRECATED_example_only",
      "keywords": [
        "legacy",
        "backward",
        "compat"
      ]
    }
  ]
}